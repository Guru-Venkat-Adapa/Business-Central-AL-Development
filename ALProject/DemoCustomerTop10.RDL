Public Function CalcPct(Amount1 as Decimal, Amount2 as Decimal) as Decimal 
    if Amount2 &lt;&gt; 0 then
       Pct = Amount1 / Amount2 * 100
    else
       Pct = 0
    end if
    REM Rounding precision = 0.1
    Return ROUND(10*Pct)/10
End Function
